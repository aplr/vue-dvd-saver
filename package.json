{
  "name": "vue-dvd-saver",
  "version": "0.1.0",
  "author": "Andreas Pfurtscheller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aplr/vue-dvd-saver/issues"
  },
  "homepage": "https://github.com/aplr/vue-dvd-saver#readme",
  "private": false,
  "main": "dist/dvdSaver.umd.js",
  "unpkg": "dist/dvdSaver.umd.min.js",
  "module": "src/ScreenSaver.vue",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name dvdSaver src/ScreenSaver.vue",
    "lint": "vue-cli-service lint",
    "prepublish": "npm run lint && npm run build"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "bootstrap-vue": "^2.5.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "svg-inline-loader": "^0.8.2",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aplr/vue-dvd-saver.git"
  },
  "files": [
    "dist/dvdSaver.umd.js",
    "dist/dvdSaver.umd.min.js",
    "dist/dvdSaver.common.js"
  ]
}
